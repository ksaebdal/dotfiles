[user]
	name = Kristian Saebdal
	email = kristian.saebdal@gmail.com
[credential]
	helper = osxkeychain
[heroku]
  account = jobb
[push]
  default = upstream
[apply]
  whitespace = nowarn
[core]
  excludesfile = ~/.gitignore
[alias]
  l = log --graph --pretty=format:'%C(yellow)%h %Cgreen%ae%Creset (%ar): %s'
  churn = !git --no-pager log --name-only --oneline | grep -v ' ' | sort | uniq -c | sort -nr | head
[branch]
  autosetuprebase = always
[color]
  diff = auto
  status = auto
  branch = auto
  ui = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[diff]
  algorithm = patience
